<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jubair.riyadh.sleepapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SleppApp">
        <activity android:name="jubair.riyadh.sleepapp.PlayerActivity"></activity>
        <activity android:name="jubair.riyadh.sleepapp.SleepDetailsActivity" />
        <activity android:name="jubair.riyadh.sleepapp.Sleep" />
        <activity android:name="jubair.riyadh.sleepapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>